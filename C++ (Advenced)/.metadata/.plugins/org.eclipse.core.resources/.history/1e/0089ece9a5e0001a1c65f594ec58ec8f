#include <iostream>
using namespace std;


template <typename T>
T test(T a){
	return a;
}


int main() {
	cout << "Template Function " << endl; // prints Template Function 

	cout << test(25) << endl;
	cout << test(25.6) << endl;
	cout << test("template") << endl;
	cout << test(-25) << endl;

	return 0;
}
