#include <iostream>
using namespace std;

class Employee{
private:
	string name;
	int salary;
public:
	Employee(string name){
		this->name = name;
	}
	Employee(string name, int salary){
		this->name = name; // this pointer her Class'a özgüdür
		this->salary = salary;
	}

	void setSalary(int salary){
		this->salary = salary;
	}

	int getSalary(){
		return salary;
	}
};

int main() {
	cout << "Constructor Overloading" << endl; // prints Constructor Overloading
	return 0;
}
